module ForLoop;

foreach = func(array, callback) {
	len = array.length()
	i = 0
	while i < len {
		callback(array[i])
		i += 1
	}
}

[0, 1, 2].foreach(func(item) {
	print "it works! : ", item
})

[1, 2, 3, 4, 5, 6].foreach(func(item) {
	print "item = ", item
})

blah = func(array, arg2) { arg2(); return array.length() / 4.0 }
print [1, 2, 3].blah(func() {
	print "hi"
})
