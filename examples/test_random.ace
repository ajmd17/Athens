module test_random {
  use library ['../acelib']

  import random
  import thread

  rgen: random::RandomGenerator

  new thread::Thread.spawn(() {
    stopwatch::start()

    i = 0
    while i < 200 {
      print rgen.next()
      i += 1
    }

    print fmt("Elapsed time: %", stopwatch::stop())
  })

  print "Generating random numbers..."
}